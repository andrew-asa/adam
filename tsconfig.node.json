{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [

    "electron.vite.config.*",
    "src/main/**/*.ts",
    "src/common/**/*.ts",
    "src/preload/*"
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "electron-vite/node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@main/*": [
        "src/main/*"
      ],
      "@resources/*": [
        "resources/*"
      ],
    },
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}